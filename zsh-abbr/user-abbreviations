abbr "..."="cd ../.."
abbr ".."="cd .."
abbr "c"="clear"
abbr "cc"="clear && printf \"\033[3J\""
abbr "cl"="claude"
abbr "cla"="claude --dangerously-skip-permissions"
abbr "co"="codex --dangerously-bypass-approvals-and-sandbox"
abbr "ex"="open ."
abbr "fdd"="fd -t d"
abbr "fddh"="fd -t d -H"
abbr "fdf"="fd -t f"
abbr "fdfh"="fd -t f -H"
abbr "gaa"="git add ."
abbr "gca"="git commit --amend"
abbr "gco"="git checkout"
abbr "gg"="git log --oneline --graph --decorate"
abbr "gp"="git push"
abbr "gpl"="git pull"
abbr "gra"="git remote add origin"
abbr "grd"="git remote remove origin"
abbr "grv"="git remote -v"
abbr "gs"="git status"
abbr "h"="history"
abbr "npr"="rm -rf node_modules package-lock.json && npm cache clean --force && npm install"
abbr "nrb"="npm run build"
abbr "nrd"="npm run dev"
abbr "o"="open"
abbr "oo"="open ."
abbr "p"="pwd"
abbr "ur"="uv run"
abbr "urm"="uv run main.py"
abbr "vs"="code"
